/* === ANIMATIONS: Master Animation Library === */
/* Consolidated and optimized animations with duplicate removal */

/* CSS Custom Properties for Common Transforms */
:root {
    --transform-reset: translateX(0px) translateY(0px) scale(1) rotate(0deg);
    --opacity-normal: 1;
    --scale-normal: 1;
    --translate-x-end: 0;
    --translate-y-end: 0;
}

/* Shimmer Animation - For loading states and attention-grabbing effects */
/* Pulse Animation - For status indicators and breathing effects */
@keyframes pulse {

    0%,
    100% {
        opacity: var(--opacity-normal);
        transform: scale(var(--scale-normal));
    }

    50% {
        opacity: 0.5;
        transform: scale(1.05);
    }
}

/* Simple Pulse Animation - For subtle status changes */
@keyframes simplePulse {

    0%,
    100% {
        opacity: var(--opacity-normal);
    }

    50% {
        opacity: 0.7;
    }
}

/* Float Animation - For floating elements with complex movement */
@keyframes float {

    0%,
    100% {
        transform: var(--transform-reset);
        opacity: 0.6;
    }

    25% {
        transform: translateX(20px) translateY(-25px) scale(1.05) rotate(2deg);
        opacity: 0.8;
    }

    50% {
        transform: translateX(-15px) translateY(-15px) scale(0.95) rotate(-1deg);
        opacity: 0.7;
    }

    75% {
        transform: translateX(-20px) translateY(20px) scale(1.02) rotate(1deg);
        opacity: 0.9;
    }
}

/* LivePulse Animation - For status dots and live indicators */
@keyframes livePulse {

    0%,
    100% {
        opacity: var(--opacity-normal);
        transform: scale(var(--scale-normal));
    }

    50% {
        opacity: 0.5;
        transform: scale(1.2);
    }
}

/* FloatUpDown Animation - Simple vertical floating */
@keyframes floatUpDown {

    0%,
    100% {
        transform: translateY(0px);
    }

    50% {
        transform: translateY(-10px);
    }
}

/* Scale Animations - Enhanced zoom-in effect */
@keyframes scaleIn {
    0% {
        opacity: 0;
        transform: scale(0.3);
    }

    50% {
        opacity: 1;
    }

    100% {
        opacity: 1;
        transform: scale(1);
    }
}

/* Slide Animations */
@keyframes slideInUp {
    from {
        opacity: 0;
        transform: translateY(20px);
    }

    to {
        opacity: var(--opacity-normal);
        transform: translateY(var(--translate-y-end));
    }
}

@keyframes slideInRight {
    from {
        transform: translateX(100%);
        opacity: 0;
    }

    to {
        transform: translateX(var(--translate-x-end));
        opacity: var(--opacity-normal);
    }
}

@keyframes fadeIn {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@keyframes fadeOut {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

@keyframes slideIn {
    from {
        opacity: 0;
        transform: translateX(100%);
    }

    to {
        opacity: 1;
        transform: translateX(0);
    }
}

@keyframes slideInLeft {
    from {
        transform: translateX(-20px);
        opacity: 0;
    }

    to {
        transform: translateX(var(--translate-x-end));
        opacity: var(--opacity-normal);
    }
}

/* Glow Animations */
@keyframes glow {

    0%,
    100% {
        box-shadow:
            0 20px 40px rgba(99, 102, 241, 0.2),
            0 0 60px rgba(99, 102, 241, 0.3),
            inset 0 1px 0 rgba(255, 255, 255, 0.3);
    }

    50% {
        box-shadow:
            0 25px 50px rgba(99, 102, 241, 0.3),
            0 0 80px rgba(99, 102, 241, 0.4),
            inset 0 1px 0 rgba(255, 255, 255, 0.4);
    }
}

/* Complex Glow Animation - Master definition for spectral glass-morphism effects (consolidates newsGlow, knowledgeGlow, etc.) */
@keyframes spectralGlow {

    0%,
    100% {
        box-shadow:
            0 20px 25px -5px rgba(0, 0, 0, 0.1),
            0 10px 10px -5px rgba(0, 0, 0, 0.04),
            0 0 0 1px rgba(255, 255, 255, 0.2),
            inset 0 1px 0 rgba(255, 255, 255, 0.4),
            0 0 60px rgba(99, 102, 241, 0.4),
            0 0 120px rgba(99, 102, 241, 0.2);
    }

    25% {
        box-shadow:
            0 25px 30px -5px rgba(0, 0, 0, 0.12),
            0 12px 12px -5px rgba(0, 0, 0, 0.06),
            0 0 0 1px rgba(255, 255, 255, 0.3),
            inset 0 1px 0 rgba(255, 255, 255, 0.5),
            0 0 80px rgba(139, 92, 246, 0.5),
            0 0 140px rgba(139, 92, 246, 0.3);
    }

    50% {
        box-shadow:
            0 22px 27px -5px rgba(0, 0, 0, 0.11),
            0 11px 11px -5px rgba(0, 0, 0, 0.05),
            0 0 0 1px rgba(255, 255, 255, 0.25),
            inset 0 1px 0 rgba(255, 255, 255, 0.45),
            0 0 70px rgba(6, 182, 212, 0.45),
            0 0 130px rgba(6, 182, 212, 0.25);
    }

    75% {
        box-shadow:
            0 24px 29px -5px rgba(0, 0, 0, 0.13),
            0 13px 13px -5px rgba(0, 0, 0, 0.07),
            0 0 0 1px rgba(255, 255, 255, 0.28),
            inset 0 1px 0 rgba(255, 255, 255, 0.48),
            0 0 75px rgba(16, 185, 129, 0.48),
            0 0 135px rgba(16, 185, 129, 0.28);
    }
}

/* Complex Background Shift Animation - Master definition for multi-layer background effects (consolidates newsBackgroundShift, knowledgeBackgroundShift, etc.) */
@keyframes spectralBackgroundShift {
    0% {
        background-position: 0% 0%, 100% 100%, 0% 50%, 0% 0%, 20% 20%, 80% 80%, 50% 50%, 10% 80%, 0% 0%, 0% 0%;
    }

    25% {
        background-position: 100% 0%, 0% 100%, 25% 50%, 0% 0%, 25% 25%, 75% 75%, 45% 55%, 15% 75%, 25% 25%, 25% 25%;
    }

    50% {
        background-position: 100% 100%, 0% 0%, 50% 50%, 0% 0%, 30% 30%, 70% 70%, 40% 60%, 20% 70%, 50% 50%, 50% 50%;
    }

    75% {
        background-position: 0% 100%, 100% 0%, 75% 50%, 0% 0%, 35% 35%, 65% 65%, 35% 65%, 25% 65%, 75% 75%, 75% 75%;
    }

    100% {
        background-position: 0% 0%, 100% 100%, 0% 50%, 0% 0%, 20% 20%, 80% 80%, 50% 50%, 10% 80%, 0% 0%, 0% 0%;
    }
}

/* Aurora Effects */
@keyframes auroraShift {

    0%,
    100% {
        background-position: 0% 50%, 100% 50%;
        opacity: 0.5;
    }

    25% {
        background-position: 100% 0%, 0% 100%;
        opacity: 0.7;
    }

    50% {
        background-position: 100% 100%, 0% 0%;
        opacity: 0.4;
    }

    75% {
        background-position: 0% 100%, 100% 0%;
        opacity: 0.6;
    }
}

/* Constellation Effects */
@keyframes constellationPulse {

    0%,
    100% {
        opacity: 0.2;
        transform: scale(1);
    }

    50% {
        opacity: 0.6;
        transform: scale(1.05);
    }
}

/* Sparkle Effects */
@keyframes sparkleAnimation {

    0%,
    100% {
        opacity: 0;
        transform: scale(0) rotate(0deg);
    }

    50% {
        opacity: 1;
        transform: scale(1.2) rotate(180deg);
    }
}

/* Wave Distortion */
@keyframes waveDistort {

    0%,
    100% {
        transform: translateX(0%) scaleX(1) scaleY(1);
        background-position: 0% 50%, 100% 30%, 50% 80%;
    }

    25% {
        transform: translateX(3%) scaleX(1.05) scaleY(0.95);
        background-position: 25% 60%, 75% 40%, 60% 70%;
    }

    50% {
        transform: translateX(-2%) scaleX(0.95) scaleY(1.05);
        background-position: 50% 70%, 50% 50%, 70% 60%;
    }

    75% {
        transform: translateX(1%) scaleX(1.02) scaleY(0.98);
        background-position: 75% 40%, 25% 60%, 40% 90%;
    }
}

/* Holographic Effects */
@keyframes holographicShimmer {
    0% {
        background-position: -40px -40px;
    }

    100% {
        background-position: 40px 40px;
    }
}

/* Energy Pulse */
@keyframes energyPulse {

    0%,
    100% {
        opacity: 0;
        transform: scale(0.3);
        border-color: rgba(99, 102, 241, 0.1);
    }

    50% {
        opacity: 0.4;
        transform: scale(1);
        border-color: rgba(99, 102, 241, 0.4);
    }
}

/* Data Pulse - Analytics-specific sophisticated pulse with box-shadow effects */
@keyframes dataPulse {

    0%,
    100% {
        opacity: 0.3;
        transform: scale(1);
        box-shadow: 0 0 10px rgba(99, 102, 241, 0.3);
    }

    50% {
        opacity: 0.8;
        transform: scale(1.3);
        box-shadow: 0 0 20px rgba(99, 102, 241, 0.8);
    }
}

/* Particle Float - Advanced particle animation with filters and complex transforms */
@keyframes particleFloat {

    0%,
    100% {
        transform: var(--transform-reset);
        opacity: 0.4;
        filter: blur(0.5px) hue-rotate(0deg);
    }

    25% {
        transform: translateX(30px) translateY(-40px) scale(1.2) rotate(90deg);
        opacity: 0.8;
        filter: blur(0.3px) hue-rotate(90deg);
    }

    50% {
        transform: translateX(-25px) translateY(-25px) scale(0.8) rotate(180deg);
        opacity: 0.6;
        filter: blur(0.7px) hue-rotate(180deg);
    }

    75% {
        transform: translateX(-35px) translateY(35px) scale(1.1) rotate(270deg);
        opacity: 0.7;
        filter: blur(0.4px) hue-rotate(270deg);
    }
}

/* Utility Classes for Common Animations */
.animate-shimmer {
    animation: shimmer 2s infinite;
}

.animate-pulse {
    animation: pulse 2s infinite;
}

.animate-float {
    animation: float 3s ease-in-out infinite;
}

.animate-glow {
    animation: glow 2s ease-in-out infinite;
}

.animate-fade-in {
    animation: fadeIn 0.5s ease-in-out;
}

.animate-slide-up {
    animation: slideInUp 0.5s ease-out;
}

.animate-scale-in {
    animation: scaleIn 0.3s ease-out;
}

/* GPU Acceleration Subtle Pulse - Master definition for smooth performance */
@keyframes subtlePulse {

    0%,
    100% {
        transform: scale(1) translateZ(0);
    }

    50% {
        transform: scale(1.003) translateZ(3px);
    }
}

/* Additional Common Animations - Consolidated from main.css duplicates */

/* Expand In Animation */
@keyframes expandIn {
    from {
        opacity: 0;
        transform: translateY(-10px);
    }

    to {
        opacity: var(--opacity-normal);
        transform: translateY(var(--translate-y-end));
    }
}

/* Rotate Animation */
@keyframes rotate {
    from {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(360deg);
    }
}

/* Breathe Animation */
@keyframes breathe {

    0%,
    100% {
        transform: scale(1);
        box-shadow: 0 0 0 0 rgba(16, 185, 129, 0.7);
    }

    25% {
        transform: scale(1.1);
        box-shadow: 0 0 0 5px rgba(16, 185, 129, 0.5);
    }

    50% {
        transform: scale(1.2);
        box-shadow: 0 0 0 10px rgba(16, 185, 129, 0.3);
    }

    75% {
        transform: scale(1.1);
        box-shadow: 0 0 0 5px rgba(16, 185, 129, 0.5);
    }
}

/* Slide Out Down Animation */
@keyframes slideOutDown {
    from {
        opacity: var(--opacity-normal);
        transform: translateY(var(--translate-y-end));
    }

    to {
        opacity: 0;
        transform: translateY(20px);
    }
}

/* Text Reveal Animation */
@keyframes textReveal {
    from {
        opacity: 0;
        background-position: 100% 0;
    }

    to {
        opacity: 1;
        background-position: 0% 0;
    }
}

/* Gradient Shift Animation - Advanced 8-layer background positioning */
@keyframes gradientShift {
    0% {
        background-position: 0% 0%, 100% 100%, 0% 50%, 0% 0%, 20% 20%, 80% 80%, 50% 50%, 0% 0%;
    }

    25% {
        background-position: 100% 0%, 0% 100%, 25% 50%, 0% 0%, 25% 25%, 75% 75%, 45% 55%, 25% 25%;
    }

    50% {
        background-position: 100% 100%, 0% 0%, 50% 50%, 0% 0%, 30% 30%, 70% 70%, 40% 60%, 50% 50%;
    }

    75% {
        background-position: 0% 100%, 100% 0%, 75% 50%, 0% 0%, 35% 35%, 65% 65%, 35% 65%, 75% 75%;
    }

    100% {
        background-position: 0% 0%, 100% 100%, 0% 50%, 0% 0%, 20% 20%, 80% 80%, 50% 50%, 0% 0%;
    }
}

/* Focus Pulse Animation */
@keyframes focusPulse {

    0%,
    100% {
        transform: scale(1);
        opacity: 1;
    }

    50% {
        transform: scale(1.05);
        opacity: 0.9;
    }
}

/* Typing Animation */
@keyframes typing {
    from {
        width: 0;
    }

    to {
        width: 100%;
    }
}

/* Blink Caret Animation */
@keyframes blink-caret {

    from,
    to {
        border-color: transparent;
    }

    50% {
        border-color: var(--text-primary);
    }
}

/* Wave Animation */
@keyframes wave {

    0%,
    100% {
        transform: rotate(0deg);
    }

    10%,
    30% {
        transform: rotate(-10deg);
    }

    20% {
        transform: rotate(10deg);
    }
}

/* Gentle Bounce Animation */
@keyframes gentle-bounce {

    0%,
    20%,
    50%,
    80%,
    100% {
        transform: translateY(0);
    }

    40% {
        transform: translateY(-10px);
    }

    60% {
        transform: translateY(-5px);
    }
}







/* Spectacular Entrance Animation - Multi-axis transforms with blur effects */
@keyframes spectacularEntrance {
    0% {
        opacity: 0;
        transform: translateY(30px) scale(0.9) rotateX(10deg);
        filter: blur(10px);
    }

    50% {
        opacity: 0.7;
        transform: translateY(-5px) scale(1.02) rotateX(-2deg);
        filter: blur(2px);
    }

    100% {
        opacity: 1;
        transform: translateY(0) scale(1) rotateX(0deg);
        filter: blur(0);
    }
}

/* Burst Particle Animation - Expanding particle effects */
@keyframes burstParticle {
    0% {
        opacity: 1;
        transform: translate(-50%, -50%) scale(0);
    }

    20% {
        opacity: 1;
        transform: translate(-50%, -50%) scale(1);
    }

    100% {
        opacity: 0;
        transform: translate(-50%, -50%) scale(0) translateX(100px) translateY(-100px);
    }
}

/* Card Sparkle Animation - 360 rotation with scale and translation */
@keyframes cardSparkle {
    0% {
        opacity: 1;
        transform: scale(0) rotate(0deg);
    }

    50% {
        opacity: 1;
        transform: scale(1.5) rotate(180deg);
    }

    100% {
        opacity: 0;
        transform: scale(0) rotate(360deg) translateY(-50px);
    }
}

/* Glow Pulse Animation - Opacity and scale transitions */
@keyframes glowPulse {

    0%,
    100% {
        opacity: 0;
        transform: scale(0.8);
    }

    50% {
        opacity: 1;
        transform: scale(1.1);
    }
}

/* Glowing Border Animation - Border color and shadow transitions */
@keyframes glowingBorder {

    0%,
    100% {
        border-color: rgba(99, 102, 241, 0.3);
        box-shadow: 0 0 5px rgba(99, 102, 241, 0.2);
    }

    50% {
        border-color: rgba(99, 102, 241, 0.8);
        box-shadow: 0 0 20px rgba(99, 102, 241, 0.6);
    }
}

/* Fade In Scale Animation - Combined fade and scale effect */
@keyframes fadeInScale {
    0% {
        opacity: 0;
        transform: scale(0.8);
    }

    /* 100% rule removed - duplicate of scaleIn keyframe */
}

/* Swing Animation - Gentle pendulum swing effect */
@keyframes swing {

    /* 0%, 100% rule removed - duplicate of wave keyframe */

    25% {
        transform: rotate(3deg);
    }

    75% {
        transform: rotate(-3deg);
    }
}

/* Ripple Effect Animation - Expanding ripple with fade out */
@keyframes ripple {
    0% {
        transform: scale(0);
        opacity: 1;
    }

    100% {
        transform: scale(4);
        opacity: 0;
    }
}

/* Color Cycle Animation - Hue rotation for dynamic color cycling */
@keyframes colorCycle {
    0% {
        filter: hue-rotate(0deg);
    }

    25% {
        filter: hue-rotate(90deg);
    }

    50% {
        filter: hue-rotate(180deg);
    }

    75% {
        filter: hue-rotate(270deg);
    }

    100% {
        filter: hue-rotate(360deg);
    }
}

/* Scale Bounce Animation - Simple scale-based bounce effect */
@keyframes scaleBounce {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(1);
    }
}

/* Slide Up Animation */
@keyframes slideUp {
    from {
        opacity: 0;
        transform: translateY(30px);
    }

    to {
        opacity: var(--opacity-normal);
        transform: translateY(var(--translate-y-end));
    }
}

/* Slide Down Animation */
@keyframes slideDown {
    from {
        opacity: var(--opacity-normal);
        transform: translateY(var(--translate-y-end));
    }

    to {
        opacity: 0;
        transform: translateY(30px);
    }
}

/* Standardized pulse animation for opacity changes - consolidates statusPulse and pulse-online */
@keyframes standardPulse {

    0%,
    100% {
        opacity: var(--opacity-normal);
    }

    50% {
        opacity: 0.5;
    }
}

/* Reduced Motion Support */
@media (prefers-reduced-motion: reduce) {

    .animate-shimmer,
    .animate-pulse,
    .animate-float,
    .animate-glow,
    .animate-fade-in,
    .animate-slide-up,
    .animate-scale-in {
        animation: none;
    }
}
